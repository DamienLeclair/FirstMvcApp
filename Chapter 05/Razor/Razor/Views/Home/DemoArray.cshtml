@model Razor.Models.Product[]

@{
    ViewBag.Title = "DemoArray";
}

@if (@Model.Any())
{
    <table>
        <thead>
            <tr><th>Property</th><th>Value</th></tr>
        </thead>
        <tbody>
            @foreach (var p in @Model)
            {
                <tr>
                    <td>@p.Name</td>
                    <td>$@p.Price</td>
                </tr>
            }
        </tbody>
    </table>
}
else
{
  <h2>No product data</h2>
}